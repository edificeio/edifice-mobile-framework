{
  "name": "appe",
  "version": "1.7.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean": "rm -r -f ./android/build && rm -r -f ./android/app/build && rm -r -f ./android/app/release && rm -r -f ./ios/build && rm -r -f ./ios/pods && rm -f ./ios/Podfile.lock && rm -r -f node_modules && rm -f package-lock.json && rm -f yarn.lock && npm cache clean --force && yarn install && cd android && ./gradlew clean && cd ../ios && pod install --repo-update && cd .. && node cli/override.js reset-cache",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "override": "node cli/override.js -l",
    "override-local": "node cli/override.js --local -l",
    "override-silent": "node cli/override.js -v --no-i $CONFIG_TO --uri=$CONFIG_URI --branch=$CONFIG_BRANCH --username=$CONFIG_USER --password=$CONFIG_PWD",
    "override-restore": "node cli/override.js restore",
    "override-restore-local": "node cli/override.js restore --local",
    "postinstall": "npx react-native-jetifier; npx patch-package",
    "prepare-alpha": "node cli/prepare-build.js alpha",
    "prepare-rc": "node cli/prepare-build.js rc",
    "prepare-major": "node cli/prepare-build.js major",
    "prepare-minor": "node cli/prepare-build.js minor",
    "prepare-rev": "node cli/prepare-build.js rev",
    "private-key": "cd android/app && rm -f release.keystore && keytool -genkey -v -keystore release.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "test": "jest",
    "tsc": "tsc -p tsconfig.json --watch"
  },
