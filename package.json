  "scripts": {
    "add-dev": "yarn add --dev --exact",
    "add-dep": "run(){ yarn add --save --exact $@ && cd ios && pod install && cd .. ; }; run",
    "android": "react-native run-android",
    "clean": "rm -r -f ./android/build && rm -r -f ./android/app/build && rm -r -f ./android/app/release && rm -r -f ./ios/build && rm -r -f ./ios/pods && rm -f ./ios/Podfile.lock && rm -r -f node_modules && rm -f package-lock.json && rm -f yarn.lock && bundle install && npm cache clean --force && yarn install && cd android && ./gradlew clean && cd ../ios && pod install --repo-update && cd .. && node cli/override.js reset-cache",
    "depcheck": "npx depcheck",
    "i18n-diff": "node cli/i18n.js diff",
    "i18n-merge-fr": "node cli/i18n.js merge fr",
    "i18n-merge-en": "node cli/i18n.js merge en",
    "i18n-merge-es": "node cli/i18n.js merge es",
    "ios": "react-native run-ios",
    "lint-all": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint-fix-all": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "lint-fix-folder": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "lint-folder": "eslint --ext .js,.jsx,.ts,.tsx",
    "override": "node cli/override.js -l",
    "override-local": "node cli/override.js --local -l",
    "override-silent": "node cli/override.js -v --no-i $CONFIG_TO --uri=$CONFIG_URI --username=$CONFIG_USER --password=$CONFIG_PWD",
    "override-restore": "node cli/override.js restore",
    "override-restore-local": "node cli/override.js restore --local",
    "patch": "npx patch-package --exclude 'nothing'",
    "postinstall": "npx react-native-jetifier; npx patch-package;",
    "prepare-alpha": "node cli/prepare-build.js alpha",
    "prepare-rc": "node cli/prepare-build.js rc",
    "prepare-major": "node cli/prepare-build.js major",
    "prepare-minor": "node cli/prepare-build.js minor",
    "prepare-rev": "node cli/prepare-build.js rev",
    "prettier": "npx prettier --write src/**/*.{ts,tsx}",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "test": "jest",
    "tsc": "tsc -p tsconfig.json",
    "scaff-module": "run(){ npx scaffolder-cli create $1 --path-prefix /src/framework/modules -f $2 moduleName=$2; }; run",
    "scaff-screen": "run(){ npx scaffolder-cli create $1 --path-prefix /src/framework/modules/$2/screens -f $3 screenName=$3 moduleName=$2 && echo \"❗️ \\033[1;33mManual steps required ! See comments in \\033[4m./src/framework/modules/$2/screens/$3/index.ts\\033[0m\"; }; run"
  },
