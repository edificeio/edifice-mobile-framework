    "i18n-js": "3.8.0",
    "mime": "3.0.0",
    "moment": "2.29.1",
    "query-string": "7.0.1",
    "querystring": "0.2.1",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-action-sheet": "2.2.0",
    "react-native-app-link": "1.0.1",
    "react-native-blob-util": "0.13.16",
    "react-native-config-reader": "4.1.1",
    "react-native-device-info": "8.4.6",
    "react-native-document-picker": "7.1.1",
    "react-native-dropdown-picker": "5.2.3",
    "react-native-fast-image": "8.5.11",
    "react-native-file-share-intent": "github:opendigitaleducation/react-native-file-share-intent",
    "react-native-file-viewer": "2.1.4",
    "react-native-flipper": "0.99.0",
    "react-native-fs": "2.18.0",
    "react-native-gesture-handler": "1.10.3",
    "react-native-global-props": "github:opendigitaleducation/react-native-global-props",
    "react-native-hide-show-password-input": "1.2.0",
    "react-native-image-picker": "4.2.0",
    "react-native-localize": "2.1.5",
    "react-native-matomo": "git+https://github.com/BonifyByForteil/react-native-matomo.git",
    "react-native-modal": "13.0.0",
    "react-native-pdf": "6.4.0",
    "react-native-permissions": "3.1.0",
    "react-native-reanimated": "2.2.4",
    "react-native-safe-area-context": "3.3.2",
    "react-native-safe-area-view": "2.0.0",
    "react-native-screens": "3.9.0",
    "react-native-share": "7.2.1",
    "react-native-snap-carousel": "3.9.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "12.2.0",
    "react-native-svg-transformer": "0.14.3",
    "react-native-swipeable": "0.6.0",
    "react-native-swiper": "1.6.0",
    "react-native-text-size": "4.0.0-rc.1",
    "react-native-tiny-toast": "1.0.7",
    "react-native-vector-icons": "9.0.0",
    "react-native-video": "5.2.0",
    "react-native-video-player": "git+https://github.com/opendigitaleducation/react-native-fullscreenvideo-player.git",
    "react-native-view-overflow": "0.0.5",
    "react-native-webview": "11.14.3",
    "react-navigation": "4.4.4",
    "react-navigation-drawer": "2.7.1",
    "react-navigation-stack": "2.10.4",
    "react-navigation-tabs": "2.11.1",
    "react-redux": "7.2.6",
    "redux": "4.1.2",
    "redux-flipper": "2.0.1",
    "redux-thunk": "2.4.0",
    "sax": "1.2.4",
    "saxophone": "0.7.2",
    "stream": "0.0.2",
    "ts-polyfill": "3.8.2",
    "unorm": "1.6.0",
    "whatwg-fetch": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/runtime": "7.16.3",
    "@jumpn/react-native-jetifier": "0.1.4",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/i18n-js": "3.8.2",
    "@types/jest": "24.0.18",
    "@types/mime": "2.0.3",
    "@types/react": "16.9.19",
    "@types/react-native": "0.63.2",
    "@types/react-native-share": "3.3.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "babel-jest": "26.6.3",
    "eslint": "7.14.0",
    "eslint-config-universe": "2.1.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.66.2",
    "patch-package": "6.4.7",
    "prettier": "1.19.1",
    "react-native-typescript-transformer": "1.2.13",
    "react-test-renderer": "17.0.2",
    "set-value": "4.1.0",
    "ts-jest": "24.0.2",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "set-value": "4.0.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "ode": {
    "appid": "com.ode.mobile",
    "appname": "app-e-dev-default",
    "properties": {
      "ios": "ios/appe/Info.plist",
      "pbxproj": "ios/appe.xcodeproj/project.pbxproj",
      "android": "android/gradle.properties"
    },
    "overrides": [
      "assets/*",
      "android/app/src/main/res/*",
      "ios/appe/Images.xcassets/*"
    ],
    "config": {
      "id": "dev-default",
      "url": "http://code.web-education.net/ODE/mobileapp-config.git",
      "branch": "master",
      "google-services.android.json": "android/app/google-services.json",
      "google-services.ios.plist": "ios/appe/GoogleService-Info.plist",
      "ode-framework-conf.ts": "ode-framework-conf.ts",
      "AppCenter-Config.plist": "ios/appe/AppCenter-Config.plist",
      "appcenter-config.json": "android/app/src/main/assets/appcenter-config.json",
      "conf/CustomTheme.ts": "src/conf/CustomTheme.ts",
      "conf/IncludedModuleConfigs.ts": "src/conf/IncludedModuleConfigs.ts",
      "conf/IncludedModules.ts": "src/conf/IncludedModules.ts",
      "conf/I18nOverride/fr.json": "src/conf/I18nOverride/fr.json",
      "conf/I18nOverride/en.json": "src/conf/I18nOverride/en.json",
      "conf/I18nOverride/es.json": "src/conf/I18nOverride/es.json"
    },
    "current": "dev-default"
  }
}
