{
  "name": "appe",
  "version": "1.2.0-SNAPSHOT",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "apk": "cd android && ./gradlew bundleRelease",
    "appcenter:build:android": "react-native bundle --platform android --dev false --entry-file 'index.js' --bundle-output 'android/app/src/main/assets/index.android.bundle' --assets-dest 'android/app/build/generated/res/react/release'",
    "appcenter:build:ios": "cd ios && pod install --repo-update && cd ../ && react-native bundle --platform ios --dev false --entry-file 'index.js' --bundle-output 'ios/appe/main.jsbundle' --assets-dest './ios'",
    "appcenter:config": "node ode-plugin.js override switch --to=$APPCENTER_TO --dontrestore --acceptall --uri=$APPCENTER_URI --branch=$APPCENTER_CONF_BRANCH --username=$APPCENTER_USERNAME --password=$APPCENTER_PWD",
    "clean": "rm -r -f ./android/app/build && rm -r -f node_modules && rm -f package-lock.json && rm -f yarn.lock && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && npm install && cd android && ./gradlew clean && cd .. ",
    "devices": "adb devices",
    "emulator": "emulator -avd emulator",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "postinstall": "npx react-native-jetifier",
    "private-key": "cd android/app && rm -f release.keystore && keytool -genkey -v -keystore release.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000",
    "start": "react-native start",
    "test": "jest",
    "tsc": "tsc -p tsconfig.json --watch"
  },
  "dependencies": {
    "@breeffy/react-native-popup-menu": "^0.8.0",
    "@react-native-community/cli": "^3.1.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "~4.1.5",
    "@react-native-community/push-notification-ios": "github:react-native-community/react-native-push-notification-ios#dcef241",
    "appcenter": "2.6.1",
    "appcenter-analytics": "2.6.1",
    "appcenter-crashes": "2.6.1",
    "base-64": "0.1.0",
    "buffer": "^5.4.3",
    "deepmerge": "4.0.0",
    "events": "3.0.0",
    "filesize": "4.1.2",
    "glamorous-native": "github:opendigitaleducation/glamorous-native",
    "hoist-non-react-statics": "^3.3.2",
    "html-entities": "1.2.1",
    "i18n-js": "~3.5.1",
    "mime": "^2.4.4",
    "moment": "2.24.0",
    "querystring": "0.2.0",
    "react": "~16.9.0",
    "react-lifecycles-compat": "^3.0.4",
    "react-native": "~0.61.5",
    "react-native-app-link": "1.0.0",
    "react-native-device-info": "^5.4.1",
    "react-native-document-picker": "~3.2.4",
    "react-native-fast-image": "^7.0.2",
    "react-native-file-share-intent": "github:opendigitaleducation/react-native-file-share-intent",
    "react-native-firebase": "^5.6.0",
    "react-native-fs": "~2.15.3",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-global-props": "github:opendigitaleducation/react-native-global-props",
    "react-native-grid-list": "~1.1.0",
    "react-native-image-picker": "1.0.1",
    "react-native-localize": "1.1.4",
    "react-native-matomo-sdk": "^0.4.0",
    "react-native-modal": "11.1.0",
    "react-native-permissions": "~2.0.3",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^3.0.3",
    "react-native-screens": "~2.0.0-alpha.34",
    "react-native-share": "~3.0.0",
    "react-native-snap-carousel": "3.8.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-swipeable": "0.6.0",
    "react-native-text-size": "^4.0.0-rc.1",
    "react-native-tiny-toast": "^1.0.6",
    "react-native-vector-icons": "6.6.0",
    "react-native-video": "github:react-native-community/react-native-video",
    "react-native-video-player": "https://github.com/opendigitaleducation/react-native-fullscreenvideo-player",
    "react-native-view-overflow": "0.0.4",
    "react-native-webview": "~7.5.1",
    "react-navigation": "^4.3.9",
    "react-navigation-drawer": "~2.3.3",
    "react-navigation-header-buttons": "^3.0.4",
    "react-navigation-stack": "^2.8.2",
    "react-navigation-tabs": "~2.7.0",
    "react-redux": "~7.1.3",
    "redux": "~4.0.5",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "^0.12.0",
    "sax": "1.2.4",
    "saxophone": "0.5.0",
    "stream": "0.0.2",
    "unorm": "1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@jumpn/react-native-jetifier": "~0.1.4",
    "@types/i18n-js": "^3.0.1",
    "@types/jest": "~24.0.18",
    "@types/mime": "~2.0.1",
    "@types/react": "~16.9.19",
    "@types/react-native": "^0.63.2",
    "@types/react-native-share": "~3.0.0",
    "@types/react-test-renderer": "~16.9.2",
    "@types/webpack-env": "~1.15.1",
    "@typescript-eslint/eslint-plugin": "~2.15.0",
    "@typescript-eslint/parser": "~2.15.0",
    "babel-jest": "^24.8.0",
    "eslint": "^6.8.0",
    "eslint-config-universe": "^2.1.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.19.1",
    "react-native-typescript-transformer": "^1.2.12",
    "react-test-renderer": "~16.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "~3.7.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "ode": {
    "appid": "com.ode.appe",
    "appname": "app-e-dev-default",
    "properties": {
      "ios": "ios/appe/Info.plist",
      "android": "android/gradle.properties"
    },
    "overrides": [
      "assets/*",
      "android/app/src/main/res/*",
      "ios/appe/Images.xcassets/*"
    ],
    "config": {
      "id": "dev-default",
      "url": "http://code.web-education.net/ODE/mobileapp-config.git",
      "branch": "master",
      "google-services.android.json": "android/app/google-services.json",
      "google-services.ios.plist": "ios/appe/GoogleService-Info.plist",
      "ode-framework-conf.ts": "ode-framework-conf.ts",
      "AppCenter-Config.plist": "ios/appe/AppCenter-Config.plist",
      "appcenter-config.json": "android/app/src/main/assets/appcenter-config.json"
    },
    "current": "dev-default"
  }
}
