{
  "name": "appe",
  "version": "0.2.12",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "apk": "cd android && ./gradlew bundleRelease",
    "appcenter:build:android": "react-native bundle --platform android --dev false --entry-file 'index.js' --bundle-output 'android/app/src/main/assets/index.android.bundle' --assets-dest 'android/app/build/generated/res/react/release'",
    "appcenter:build:ios": "react-native bundle --platform ios --dev false --entry-file 'index.js' --bundle-output 'ios/appe/main.jsbundle' --assets-dest './ios'",
    "appcenter:config": "node ode-plugin.js override switch --to=$APPCENTER_TO --dontrestore --acceptall --uri=$APPCENTER_URI --branch=$APPCENTER_CONF_BRANCH --username=$APPCENTER_USERNAME --password=$APPCENTER_PWD",
    "clean": "rm -r -f ./android/app/build && rm -r -f node_modules && rm -f package-lock.json && rm -f yarn.lock && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && npm install && cd android && ./gradlew clean && cd .. ",
    "devices": "adb devices",
    "emulator": "emulator -avd emulator",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "postinstall": "npx react-native-jetifier",
    "private-key": "cd android/app && rm -f release.keystore && keytool -genkey -v -keystore release.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "tsc": "tsc -p tsconfig.json --watch"
  },
  "dependencies": {
    "@react-native-community/netinfo": "4.1.2",
    "@react-native-community/push-notification-ios": "github:react-native-community/react-native-push-notification-ios#dcef241",
    "appcenter": "^2.4.0",
    "appcenter-analytics": "^2.4.0",
    "appcenter-crashes": "^2.4.0",
    "base-64": "0.1.0",
    "buffer": "5.2.1",
    "deepmerge": "4.0.0",
    "events": "3.0.0",
    "filesize": "4.1.2",
    "glamorous-native": "github:opendigitaleducation/glamorous-native",
    "html-entities": "1.2.1",
    "i18n-js": "3.3.0",
    "mime": "^2.4.4",
    "moment": "2.24.0",
    "querystring": "0.2.0",
    "react": "16.8.6",
    "react-lifecycles-compat": "^3.0.4",
    "react-native": "0.60.3",
    "react-native-app-link": "1.0.0",
    "react-native-device-info": "2.2.2",
    "react-native-fast-image": "7.0.2",
    "react-native-firebase": "5.5.5",
    "react-native-fs": "2.13.3",
    "react-native-gesture-handler": "~1.4.1",
    "react-native-global-props": "github:opendigitaleducation/react-native-global-props",
    "react-native-image-picker": "1.0.1",
    "react-native-localize": "1.1.4",
    "react-native-mixpanel": "1.1.4",
    "react-native-modal": "11.1.0",
    "react-native-permissions": "1.1.1",
    "react-native-snap-carousel": "3.8.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "^9.13.3",
    "react-native-swipeable": "0.6.0",
    "react-native-vector-icons": "6.6.0",
    "react-native-video": "4.4.4",
    "react-native-video-controls": "2.2.3",
    "react-native-view-overflow": "0.0.4",
    "react-native-webview": "5.12.1",
    "react-navigation": "3.13.0",
    "react-redux": "7.1.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "^0.11.2",
    "sax": "1.2.4",
    "saxophone": "0.5.0",
    "stream": "0.0.2",
    "unorm": "1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/runtime": "^7.5.4",
    "@jumpn/react-native-jetifier": "^0.1.4",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/core-js": "^2.5.2",
    "@types/i18n-js": "^3.0.1",
    "@types/node": "^12.7.11",
    "@types/react-navigation": "^3.0.8",
    "@types/react-redux": "^7.1.4",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.14.0",
    "@types/jest": "^24.0.18",
    "@types/mime": "^2.0.1",
    "@types/react": "^16.9.5",
    "@types/react-native": "^0.60.0",
    "@types/react-test-renderer": "^16.8.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.0.1",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-native-typescript-transformer": "^1.2.12",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "ode": {
    "appid": "com.ode.appe",
    "appname": "app-e-dev-default",
    "properties": {
      "ios": "ios/appe/Info.plist",
      "android": "android/gradle.properties"
    },
    "overrides": [
      "assets/*",
      "android/app/src/main/res/*",
      "ios/appe/Images.xcassets/*"
    ],
    "config": {
      "id": "dev-default",
      "url": "http://code.web-education.net/ODE/mobileapp-config.git",
      "branch": "x64",
      "google-services.android.json": "android/app/google-services.json",
      "google-services.ios.plist": "ios/appe/GoogleService-Info.plist",
      "ode-framework-conf.ts": "ode-framework-conf.ts",
      "AppCenter-Config.plist": "ios/appe/AppCenter-Config.plist",
      "appcenter-config.json": "android/app/src/main/assets/appcenter-config.json"
    },
    "current": "dev-default"
  }
}
