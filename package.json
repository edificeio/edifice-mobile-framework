{
  "name": "appe",
  "version": "1.5.2-indev.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "apk": "cd android && ./gradlew bundleRelease",
    "appcenter:build:android": "react-native bundle --platform android --dev false --entry-file 'index.js' --bundle-output 'android/app/src/main/assets/index.android.bundle' --assets-dest 'android/app/build/generated/res/react/release'",
    "appcenter:build:ios": "cd ios && pod install --repo-update && cd ../ && react-native bundle --platform ios --dev false --entry-file 'index.js' --bundle-output 'ios/appe/main.jsbundle' --assets-dest './ios'",
    "appcenter:config": "node ode-plugin.js override switch --to=$APPCENTER_TO --dontrestore --acceptall --uri=$APPCENTER_URI --branch=$APPCENTER_CONF_BRANCH --username=$APPCENTER_USERNAME --password=$APPCENTER_PWD",
    "clean": "rm -r -f ./android/build && rm -r -f ./android/app/build && rm -r -f ./ios/pods && rm -f ./ios/Podfile.lock && rm -r -f node_modules && rm -f package-lock.json && npm install && cd android && ./gradlew clean && cd ../ios && pod install && cd ..",
    "devices": "adb devices",
    "emulator": "emulator -avd emulator",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "postinstall": "npx react-native-jetifier; npx patch-package",
    "private-key": "cd android/app && rm -f release.keystore && keytool -genkey -v -keystore release.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000",
    "start": "react-native start",
    "test": "jest",
    "tsc": "tsc -p tsconfig.json --watch"
  },
  "dependencies": {
    "@emotion/native": "11.0.0",
    "@emotion/react": "11.0.0",
    "@flyerhq/react-native-android-uri-path": "2.1.0",
    "@react-native-async-storage/async-storage": "1.15.5",
    "@react-native-community/cli": "5.0.1",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/netinfo": "6.0.0",
    "@react-native-community/progress-bar-android": "1.0.4",
    "@react-native-community/progress-view": "1.3.1",
    "@react-native-community/push-notification-ios": "1.8.0",
    "@react-native-firebase/app": "12.0.0",
    "@react-native-firebase/messaging": "12.0.0",
    "@react-native-picker/picker": "1.16.1",
    "appcenter": "4.1.0",
    "appcenter-analytics": "4.1.0",
    "appcenter-crashes": "4.1.0",
    "base-64": "1.0.0",
    "buffer": "6.0.3",
    "deepmerge": "4.2.2",
    "events": "3.3.0",
    "filesize": "6.3.0",
    "flat": "5.0.2",
    "glamorous-native": "github:opendigitaleducation/glamorous-native",
    "hoist-non-react-statics": "3.3.2",
    "html-entities": "1.4.0",
    "i18n-js": "3.8.0",
    "mime": "2.5.2",
    "moment": "2.29.1",
    "querystring": "0.2.1",
    "react": "17.0.2",
    "react-native": "0.64.2",
    "react-native-action-sheet": "2.2.0",
    "react-native-app-link": "1.0.0",
    "react-native-device-info": "8.1.3",
    "react-native-document-picker": "5.2.0",
    "react-native-fast-image": "8.3.4",
    "react-native-file-share-intent": "github:opendigitaleducation/react-native-file-share-intent",
    "react-native-gesture-handler": "1.10.3",
    "react-native-global-props": "github:opendigitaleducation/react-native-global-props",
    "react-native-image-picker": "4.0.3",
    "react-native-localize": "2.1.1",
    "react-native-matomo": "git+https://github.com/BonifyByForteil/react-native-matomo.git",
    "react-native-modal": "11.10.0",
    "react-native-pdf": "6.3.0",
    "react-native-permissions": "3.0.4",
    "react-native-reanimated": "2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-safe-area-view": "2.0.0",
    "react-native-screens": "3.3.0",
    "react-native-share": "6.2.1",
    "react-native-snap-carousel": "3.9.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-swipeable": "0.6.0",
    "react-native-text-size": "4.0.0-rc.1",
    "react-native-tiny-toast": "1.0.7",
    "react-native-vector-icons": "8.1.0",
    "react-native-video": "5.1.1",
    "react-native-video-player": "git+https://github.com/opendigitaleducation/react-native-fullscreenvideo-player.git",
    "react-native-view-overflow": "0.0.5",
    "react-native-webview": "11.6.4",
    "react-navigation": "4.4.3",
    "react-navigation-drawer": "2.3.3",
    "react-navigation-stack": "2.10.1",
    "react-navigation-tabs": "2.10.1",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "0.12.0",
    "sax": "1.2.4",
    "saxophone": "0.7.1",
    "stream": "0.0.2",
    "ts-polyfill": "3.8.2",
    "unorm": "1.6.0",
    "whatwg-fetch": "3.6.2"
  },
  "resolutions": {
    "react-native-safe-area-view": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/runtime": "7.14.0",
    "@jumpn/react-native-jetifier": "0.1.4",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/i18n-js": "3.8.1",
    "@types/jest": "26.0.23",
    "@types/mime": "2.0.3",
    "@types/react": "17.0.9",
    "@types/react-native": "0.64.8",
    "@types/react-native-share": "3.3.2",
    "@types/react-test-renderer": "17.0.1",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "babel-jest": "27.0.2",
    "eslint": "7.27.0",
    "eslint-config-universe": "7.0.1",
    "jest": "27.0.3",
    "metro-react-native-babel-preset": "0.66.0",
    "patch-package": "6.4.7",
    "prettier": "2.3.0",
    "react-native-typescript-transformer": "1.2.13",
    "react-test-renderer": "17.0.2",
    "ts-jest": "27.0.2",
    "typescript": "4.3.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "ode": {
    "appid": "com.ode.appe",
    "appname": "app-e-dev-default",
    "properties": {
      "ios": "ios/appe/Info.plist",
      "android": "android/gradle.properties"
    },
    "overrides": [
      "assets/*",
      "android/app/src/main/res/*",
      "ios/appe/Images.xcassets/*"
    ],
    "config": {
      "id": "dev-default",
      "url": "http://code.web-education.net/ODE/mobileapp-config.git",
      "branch": "master",
      "google-services.android.json": "android/app/google-services.json",
      "google-services.ios.plist": "ios/appe/GoogleService-Info.plist",
      "ode-framework-conf.ts": "ode-framework-conf.ts",
      "AppCenter-Config.plist": "ios/appe/AppCenter-Config.plist",
      "appcenter-config.json": "android/app/src/main/assets/appcenter-config.json",
      "conf/CustomTheme.ts": "src/conf/CustomTheme.ts",
      "conf/IncludedModuleConfigs.ts": "src/conf/IncludedModuleConfigs.ts",
      "conf/IncludedModules.ts": "src/conf/IncludedModules.ts",
      "conf/I18nOverride/fr.json": "src/conf/I18nOverride/fr.json",
      "conf/I18nOverride/en.json": "src/conf/I18nOverride/en.json",
      "conf/I18nOverride/es.json": "src/conf/I18nOverride/es.json"
    },
    "current": "dev-default"
  }
}
