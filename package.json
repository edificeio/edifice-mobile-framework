{
  "name": "appe",
  "version": "1.6.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean": "rm -r -f ./android/build && rm -r -f ./android/app/build && rm -r -f ./ios/build && rm -r -f ./ios/pods && rm -f ./ios/Podfile.lock && rm -r -f node_modules && rm -f package-lock.json && rm -f yarn.lock && npm cache clean --force && yarn install && cd android && ./gradlew clean && cd ../ios && pod install --repo-update && cd ..",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "postinstall": "npx react-native-jetifier; npx patch-package",
    "private-key": "cd android/app && rm -f release.keystore && keytool -genkey -v -keystore release.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "test": "jest",
    "tsc": "tsc -p tsconfig.json --watch",
    "prepare-alpha": "node prepare-build.js alpha",
    "prepare-rc": "node prepare-build.js rc",
    "prepare-major": "node prepare-build.js major",
    "prepare-minor": "node prepare-build.js minor",
    "prepare-rev": "node prepare-build.js rev",
    "switch-config": "node ode-plugin.js override switch --to=$CONFIG_TO --dontrestore --acceptall --uri=$CONFIG_URI --branch=$CONFIG_BRANCH --username=$CONFIG_USER --password=$CONFIG_PWD"
  },
  "dependencies": {
    "@emotion/native": "11.0.0",
    "@emotion/react": "11.5.0",
    "@flyerhq/react-native-android-uri-path": "2.2.2",
    "@react-native-async-storage/async-storage": "1.15.11",
    "@react-native-community/cli": "6.1.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/netinfo": "6.1.0",
    "@react-native-community/progress-bar-android": "1.0.4",
    "@react-native-community/progress-view": "1.3.1",
    "@react-native-community/push-notification-ios": "1.10.1",
    "@react-native-firebase/app": "13.0.1",
    "@react-native-firebase/messaging": "13.0.1",
    "@react-native-picker/picker": "2.2.0",
    "@skele/components": "1.0.0-alpha.40",
    "appcenter": "4.3.0",
    "appcenter-analytics": "4.3.0",
    "appcenter-crashes": "4.3.0",
    "base-64": "1.0.0",
    "buffer": "6.0.3",
    "deepmerge": "4.2.2",
    "events": "3.3.0",
    "filesize": "8.0.6",
    "flat": "5.0.2",
    "glamorous-native": "github:opendigitaleducation/glamorous-native",
